{
  "name": "league-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "build": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "migrate:prod": "NODE_ENV=production pnpm exec prisma migrate deploy",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:coverage:lcov": "vitest run --coverage --coverage.reporter=lcov && vitest run --coverage --coverage.reporter=lcov --config vitest.api.config.mts",
    "test:api": "vitest run --config vitest.api.config.mts",
    "test:api:watch": "vitest --config vitest.api.config.mts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:cov": "vitest run --coverage --coverage.reportOnFailure --coverage.reporter=text-summary --coverage.reporter=text && vitest run --coverage --coverage.reportOnFailure --coverage.reporter=text-summary --coverage.reporter=text --config vitest.api.config.mts",
    "test:all": "pnpm run test:unit && pnpm run test:api && pnpm run test:e2e",
    "test:ci": "vitest run --coverage && playwright test",
    "debt:check": "ts-node -r tsconfig-paths/register scripts/debt-marker-check.ts",
    "debt:fix": "ts-node -r tsconfig-paths/register scripts/debt-marker-fix.ts",
    "test:decodo": "ts-node -r tsconfig-paths/register scripts/test-decodo-scraper.ts",
    "setup:hooks": "bash scripts/setup-git-hooks.sh",
    "prepare": "husky install",
    "docker:build": "docker build -t league-api .",
    "docker:run": "docker run --rm --env-file .env -p 3000:3000 league-api",
    "docker:test": "pnpm run docker:build && docker run --rm --env-file .env -p 3000:3000 --health-cmd='wget --no-verbose --tries=1 --spider http://localhost:3000/health || exit 1' --health-interval=10s --health-timeout=5s --health-retries=3 league-api"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "tests/**/*.{ts,js}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "tests/api/**/*.{ts,js}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ],
    "tests/e2e/**/*.{ts,js}": [
      "eslint --fix --max-warnings 0",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@bull-board/api": "^6.14.0",
    "@bull-board/express": "^6.14.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.18.0",
    "axios": "^1.12.2",
    "bullmq": "^5.63.0",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.1",
    "mathjs": "^13.0.0",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.0.0",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.4.5",
    "cron": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@darraghor/eslint-plugin-nestjs-typed": "^7.1.12",
    "@elsikora/eslint-plugin-nestjs-typed": "^3.0.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@playwright/test": "^1.48.0",
    "@suites/di.nestjs": "^3.0.1",
    "@suites/doubles.vitest": "^3.0.1",
    "@suites/unit": "^3.0.1",
    "@swc/core": "^1.15.7",
    "@trilon/eslint-plugin": "^0.2.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-security-node": "^1.1.4",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "playwright": "^1.48.0",
    "prettier": "^3.4.2",
    "prisma": "^6.18.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-swc": "^1.5.9",
    "vitest": "^2.1.8"
  }
}
