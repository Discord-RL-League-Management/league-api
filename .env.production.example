# Production Environment Configuration
# Copy this file to .env.production and fill in your production values

# Application
NODE_ENV=production
PORT=3000

# Database
DATABASE_URL="postgresql://username:password@host:5432/database_name?schema=public"

# Bot Authentication
BOT_API_KEY=your-production-bot-api-key-here
API_KEY_SALT=your-production-api-key-salt-here

# JWT Authentication
JWT_SECRET=your-production-jwt-secret-64-characters-minimum
JWT_EXPIRES_IN=7d

# Discord OAuth
DISCORD_CLIENT_ID=your-production-discord-client-id
DISCORD_CLIENT_SECRET=your-production-discord-client-secret
DISCORD_CALLBACK_URL=https://your-api-domain.com/auth/discord/callback

# Frontend
FRONTEND_URL=https://your-frontend-domain.com

# Rate Limiting (optional - defaults provided)
THROTTLE_TTL=60000
THROTTLE_LIMIT=100

# New Relic Logs API Configuration
NEW_RELIC_LICENSE_KEY=your-production-license-key-here
NEW_RELIC_ENABLED=true












