PORT=3000
NODE_ENV=development
BOT_API_KEY=your-bot-api-key-here
API_KEY_SALT=your-api-key-salt-here
FRONTEND_URL=http://localhost:5173
DATABASE_URL="postgresql://league_user:league_pass@localhost:5432/league_db?schema=public"

# Discord OAuth
DISCORD_CLIENT_ID=your-discord-client-id-here
DISCORD_CLIENT_SECRET=your-discord-client-secret-here
DISCORD_CALLBACK_URL=http://localhost:3000/auth/discord/callback

# JWT configuration
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRES_IN=7d

# Token encryption (Phase 9)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=your-64-character-hex-encryption-key-here

# Discord Bot Token (Phase 10)
# Required for Discord API validation - this is DIFFERENT from DISCORD_TOKEN in discord-bot/.env
DISCORD_BOT_TOKEN=your-discord-bot-token-here

# Discord API Configuration (optional - has defaults)
DISCORD_API_URL=your-discord-api-url-here
DISCORD_TIMEOUT=10000
DISCORD_RETRY_ATTEMPTS=3

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password-here
REDIS_DB=0

# BullMQ Queue Configuration
QUEUE_CONCURRENCY=5
QUEUE_DEFAULT_JOB_OPTIONS={"removeOnComplete": 100, "removeOnFail": 50, "attempts": 3, "backoff": {"type": "exponential", "delay": 2000}}

# Zyte API Configuration
ZYTE_API_KEY=your-zyte-api-key-here
ZYTE_PROXY_HOST=your-zyte-proxy-host-here
ZYTE_PROXY_PORT=your-zyte-proxy-port-here
ZYTE_RATE_LIMIT_PER_MINUTE=60
ZYTE_TIMEOUT_MS=30000
ZYTE_RETRY_ATTEMPTS=3
ZYTE_RETRY_DELAY_MS=1000

# Tracker Refresh Configuration
TRACKER_REFRESH_INTERVAL_HOURS=24
TRACKER_BATCH_SIZE=100
TRACKER_REFRESH_CRON=0 2 * * *
TRACKER_MAX_SCRAPING_ATTEMPTS=3

# New Relic Logging Credentials
NEW_RELIC_API_KEY=your-new-relic-key-here
